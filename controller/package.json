{
  "name": "ecmwf-dataset-crawl-api",
  "version": "0.1.0",
  "description": "Controller + API for ecmwf-dataset-crawl",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "env $(cat .env | grep -v ^# | xargs) node --inspect",
    "build": "rimraf dist && tsc -p . || true",
    "build:watch": "rimraf dist && tsc -p . -w || true",
    "dev": "NODE_ENV=development; nodemon -e js,yaml --inspect",
    "lint": "eslint ."
  },
  "keywords": [
    "swagger"
  ],
  "license": "Apache-2.0",
  "private": true,
  "dependencies": {
    "connect": "~3.6.6",
    "elasticsearch": "^15.0.0",
    "js-yaml": "~3.11.0",
    "swagger-tools": "0.10.3"
  },
  "devDependencies": {
    "@types/node": "^10.1.2",
    "nodemon": "^1.17.5",
    "rimraf": "^2.6.2",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  }
}
